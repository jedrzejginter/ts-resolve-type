{
  "name": "ts-resolve-type",
  "version": "0.1.0-alpha.2",
  "description": "Get resolved TypeScript type",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "bin": "./dist/cli.js",
  "files": [
    "dist"
  ],
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "pkgroll",
    "prepack": "npm run build"
  },
  "keywords": [
    "typescript",
    "type",
    "resolve"
  ],
  "author": "JÄ™drzej Ginter <hello@jedrzejginter.com>",
  "sideEffects": false,
  "repository": "jedrzejginter/ts-resolve-type",
  "license": "MIT",
  "engines": {
    "node": ">= 16"
  },
  "peerDependencies": {
    "typescript": "^5 || ^6"
  },
  "devDependencies": {
    "@tsconfig/strictest": "2.0.2",
    "@types/node": "20.10.0",
    "pkgroll": "2.0.1",
    "typescript": "5.3.2"
  }
}
